//difference between while and do while maen
using System;
class Program {
  public static void Main (string[] args) {

    int i=2;
      do{
        Console.WriteLine(i);
        i +=2;
      }while(i<=20);

    int n=2;
    while(n<=20){
      Console.WriteLine(n);
      n+=2;
    }

    for(int t=2;t<=20; t +=2){
      Console.WriteLine(t);
    }Console.WriteLine("Done");
    
   
    
