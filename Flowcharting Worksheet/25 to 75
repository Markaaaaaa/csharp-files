using System;

class Program {
  public static void Main (string[] args) {
    int n=24, sum=0;
    while(n<=74)
    {
      n=n+1;
      sum=sum+n;
    }
    Console.WriteLine (sum);
    
    
    int sum=0;
    for(int n=24;n<=74;n=n+1)
    {
      sum=sum+n;
    }
    Console.WriteLine (sum);
  }
}
